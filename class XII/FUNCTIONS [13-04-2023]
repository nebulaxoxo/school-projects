# Function to check if a given number x is prime or not
def isprime(x):
    for i in range(2, x):
        if x%i == 0:
            print(x, 'is not prime.')
            break
        else:
            print(x, 'is prime.')
            break

isprime(22)
isprime(13)

# Function to print multiplication of table of x up till y
def gen_table(x,y):
    for i in range (1, y+1):
        print(x, '*', i, '=', x*i)
