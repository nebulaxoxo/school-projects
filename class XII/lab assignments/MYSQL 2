USE assignments;
CREATE TABLE Faculty(
    F_ID VARCHAR(3) PRIMARY KEY,
    Fname VARCHAR(10) NOT NULL,
    Lname VARCHAR(10) NOT NULL,
    Hire_Date DATE,
    Salary FLOAT);

